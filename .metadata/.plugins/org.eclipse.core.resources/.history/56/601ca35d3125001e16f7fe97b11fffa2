package exam;

public class SecondHighest {
	public static void main(String[] args) {
		/*
		 * int arr[] = { 100, 14, 85, 99, 55 }; int temp;
		 * 
		 * for (int i = 0; i < arr.length; i++) { for (int j = i + 1; j < arr.length;
		 * j++) { if (arr[i] > arr[j]) { temp = arr[i]; arr[i] = arr[j]; arr[j] = temp;
		 * } } }
		 * 
		 * 
		 * System.out.println("Second highest element is " + arr[arr.length - 2]);
		 */
		  int[] a = { 100,14,46,47,94,94,52,86,36,94,89 };
	        int highest = Integer.MIN_VALUE;
	        int secondhighest = Integer.MIN_VALUE;
	        int temp;

	        for (int i = 0; i < a.length; i++) {
	            if (a[i] > highest) {
	                temp = highest;
	                highest = a[i];
	                secondhighest = temp;
	            } 
	            else if (a[i] > secondhighest && a[i] != highest) {
	            	secondhighest = a[i];
	            }
	        }

	            System.out.println(secondhighest);
	        

	}
}