package exam;

public class SecondHighest {
	public static void main(String[] args) {
		int arr[]= {100,14,85,99,55};
		int temp;
		/*
		 * int max=arr[arr.length]; int secondmax=arr[arr.length-1];
		 */
		 for(int i=0;i<arr.length;i++) {
			 for(int j=i+1;j<arr.length;j++) {
			 if(arr[i]>arr[j]) {
				 temp=arr[i];
				 arr[i]=arr[j];
				 arr[j]=temp;
			 }
			 }
		 }
		 for(int i=0;i<arr.length;i++) {
			 System.out.println(arr[i]);
		 }
		 //System.out.println("second highest element is "+ arr[arr.length-1]);

	}
}
