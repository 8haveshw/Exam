package exam;

public class SecondHighest {
	public static void main(String[] args) {
		/*
		 * int arr[] = { 100, 14, 85, 99, 55 }; int temp;
		 * 
		 * for (int i = 0; i < arr.length; i++) { for (int j = i + 1; j < arr.length;
		 * j++) { if (arr[i] > arr[j]) { temp = arr[i]; arr[i] = arr[j]; arr[j] = temp;
		 * } } }
		 * 
		 * 
		 * System.out.println("Second highest element is " + arr[arr.length - 2]);
		 */
		  int[] a = { 6, 8, 2, 4, 3, 1, 5, 7,8 };
	        int max = Integer.MIN_VALUE;
	        int second_max = Integer.MIN_VALUE;
	        int temp;

	        for (int i = 0; i < a.length; i++) {
	            if (a[i] > max) {
	                temp = max;
	                max = a[i];
	                second_max = temp;
	            } else if (a[i] > second_max && a[i] != max) {
	                second_max = a[i];
	            }
	        }
	        if (second_max == Integer.MIN_VALUE) {
	            System.out.println("No Second Highest value");
	        } else {
	            System.out.println(second_max);
	        }

	}
}